    export compartment_id=<substitute\-value\-of\-compartment_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/cloud\-bridge/ocb\-agent\-svc/environment/create.html#cmdoption\-compartment\-id
    export dependency_name=<substitute\-value\-of\-dependency_name> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/cloud\-bridge/ocb\-agent\-svc/agent\-dependency/create.html#cmdoption\-dependency\-name
