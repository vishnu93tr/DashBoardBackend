    export deployment_id=<substitute\-value\-of\-deployment_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/goldengate/certificate/create.html#cmdoption\-deployment\-id
