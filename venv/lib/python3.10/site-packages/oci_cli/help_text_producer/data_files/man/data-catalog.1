    export data_asset_key=<substitute\-value\-of\-data_asset_key> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/data\-catalog/entity/get.html#cmdoption\-data\-asset\-key
