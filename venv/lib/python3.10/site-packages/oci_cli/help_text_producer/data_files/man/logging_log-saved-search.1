    export log_query=<substitute\-value\-of\-log_query> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/logging/log\-saved\-search/create.html#cmdoption\-log\-query
