.sp
.nf
.ft C
    export compartment_id=<substitute\-value\-of\-compartment_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/fleet\-software\-update/fsu\-collection/create.html#cmdoption\-compartment\-id
    export service_type=<substitute\-value\-of\-service_type> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/fleet\-software\-update/fsu\-collection/create.html#cmdoption\-service\-type
    export type=<substitute\-value\-of\-type> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/fleet\-software\-update/fsu\-collection/create.html#cmdoption\-type
    export gvd_version=<substitute\-value\-of\-gvd_version> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/fleet\-software\-update/fsu\-cycle/update\-version\-target.html#cmdoption\-gvd\-version

