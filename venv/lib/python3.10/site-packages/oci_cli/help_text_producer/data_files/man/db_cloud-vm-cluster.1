    export hostname=<substitute\-value\-of\-hostname> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/db/cloud\-vm\-cluster/create.html#cmdoption\-hostname
