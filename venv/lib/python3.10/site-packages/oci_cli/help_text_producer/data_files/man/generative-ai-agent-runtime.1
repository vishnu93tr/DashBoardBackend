.INDENT 3.5
.sp
.nf
.ft C
    export agent_endpoint_id=<substitute\-value\-of\-agent_endpoint_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/generative\-ai\-agent\-runtime/session/create.html#cmdoption\-agent\-endpoint\-id

    session_id=$(oci generative\-ai\-agent\-runtime session create \-\-agent\-endpoint\-id $agent_endpoint_id \-\-query data.id \-\-raw\-output)

    oci generative\-ai\-agent\-runtime session get \-\-agent\-endpoint\-id $agent_endpoint_id \-\-session\-id $session_id
.ft P
.fi
