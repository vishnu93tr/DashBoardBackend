    export compartment_id=<substitute\-value\-of\-compartment_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/resource\-scheduler/schedule/create.html#cmdoption\-compartment\-id
    export recurrence_details=<substitute\-value\-of\-recurrence_details> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/resource\-scheduler/schedule/create.html#cmdoption\-recurrence\-details
    export recurrence_type=<substitute\-value\-of\-recurrence_type> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/resource\-scheduler/schedule/create.html#cmdoption\-recurrence\-type

    schedule_id=$(oci resource\-scheduler schedule create \-\-action $action \-\-compartment\-id $compartment_id \-\-recurrence\-details $recurrence_details \-\-recurrence\-type $recurrence_type \-\-query data.id \-\-raw\-output)

    oci resource\-scheduler schedule get \-\-schedule\-id $schedule_id
.ft P
.fi
