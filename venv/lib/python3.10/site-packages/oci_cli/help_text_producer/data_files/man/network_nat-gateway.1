    vcn_id=$(oci network vcn create \-\-cidr\-block $cidr_block \-\-compartment\-id $compartment_id \-\-query data.id \-\-raw\-output)

    nat_gateway_id=$(oci network nat\-gateway create \-\-compartment\-id $compartment_id \-\-vcn\-id $vcn_id \-\-query data.id \-\-raw\-output)

