.nf
.ft C
    export compartment_id=<substitute\-value\-of\-compartment_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/organizations/domain/create.html#cmdoption\-compartment\-id
    export domain_name=<substitute\-value\-of\-domain_name> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/organizations/domain/create.html#cmdoption\-domain\-name
    export ons_subscription_id=<substitute\-value\-of\-ons_subscription_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/organizations/domain\-governance/create.html#cmdoption\-ons\-subscription\-id
    export ons_topic_id=<substitute\-value\-of\-ons_topic_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/organizations/domain\-governance/create.html#cmdoption\-ons\-topic\-id
    export subscription_email=<substitute\-value\-of\-subscription_email> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/organizations/domain\-governance/create.html#cmdoption\-subscription\-email

    domain_id=$(oci organizations domain create \-\-compartment\-id $compartment_id \-\-domain\-name $domain_name \-\-query data.id \-\-raw\-output)

    domain_governance_id=$(oci organizations domain\-governance create \-\-compartment\-id $compartment_id \-\-domain\-id $domain_id \-\-ons\-subscription\-id $ons_subscription_id \-\-ons\-topic\-id $ons_topic_id \-\-subscription\-email $subscription_email \-\-query data.id \-\-raw\-output)

    oci organizations domain\-governance update \-\-domain\-governance\-id $domain_governance_id
