    export job_execution_key=<substitute\-value\-of\-job_execution_key> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/data\-catalog/job\-metric/get.html#cmdoption\-job\-execution\-key
    export job_key=<substitute\-value\-of\-job_key> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/data\-catalog/job\-metric/get.html#cmdoption\-job\-key
    export job_metrics_key=<substitute\-value\-of\-job_metrics_key> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/data\-catalog/job\-metric/get.html#cmdoption\-job\-metrics\-key

    catalog_id=$(oci data\-catalog catalog create \-\-compartment\-id $compartment_id \-\-query data.id \-\-raw\-output)

    oci data\-catalog job\-metric get \-\-catalog\-id $catalog_id \-\-job\-execution\-key $job_execution_key \-\-job\-key $job_key \-\-job\-metrics\-key $job_metrics_key
.ft P
.fi
