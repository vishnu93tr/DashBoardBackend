    export default_external_table_location=<substitute\-value\-of\-default_external_table_location> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/data\-catalog/metastore/create.html#cmdoption\-default\-external\-table\-location
