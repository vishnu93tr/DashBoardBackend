.nf
.ft C
    export compartment_id=<substitute\-value\-of\-compartment_id> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/database\-management/managed\-database\-group/create.html#cmdoption\-compartment\-id
    export name=<substitute\-value\-of\-name> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/database\-management/managed\-database\-group/create.html#cmdoption\-name

    managed_database_group_id=$(oci database\-management managed\-database\-group create \-\-compartment\-id $compartment_id \-\-name $name \-\-query data.id \-\-raw\-output)

    oci database\-management managed\-database\-group update \-\-managed\-database\-group\-id $managed_database_group_id
.ft P
