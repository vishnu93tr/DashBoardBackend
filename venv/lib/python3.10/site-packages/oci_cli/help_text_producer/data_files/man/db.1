    export cpu_core_count=<substitute\-value\-of\-cpu_core_count> # https://docs.cloud.oracle.com/en\-us/iaas/tools/oci\-cli/latest/oci_cli_docs/cmdref/db/cloud\-vm\-cluster/create.html#cmdoption\-cpu\-core\-count
